<div class="row">
      <header class='teal center-align z-depth-2 negate_margin col s9 offset-s3'>
        <h2 class='teal center-align negate_margin'>Welcome to Netflixxx And Chill!</h2>
      </header>
      <div class="teal lighten-5 col s9 offset-s3">
        <div class='row'>
          <div class='col s3 offset-s9'>
            <%= button_to 'Edit Profile', edit_user_registration_path(current_user), method: :get, class: 'btn' %>
          </div>
        </div>
        <div class='row'>
          <div class='col s6'>
            <h4>First Name: </h4>
            <h5><%= current_user.first_name %></h5>
          </div>
          <div class='col s6'>
            <h4>Last Name: </h4>
            <h5><%= current_user.last_name %></h5>
          </div>
        </div>
        <div class='row'>
          <div class='col s6'>
            <h4>Email Address: </h4>
            <h5><%= current_user.email %></h5>
          </div>
          <div class='col s6'>
            <h4>Age: </h4>
            <h5><%= current_user.age %></h5>
          </div>
        </div>
        <div class='row'>
          <div class='col s6'>
            <h4>Relationship Status: </h4>
            <%= current_user.status %></h5>
          </div>
          <div class='col s6'>
            <h4>Sex: </h4>
            <h5><%= current_user.sex %></h5>
          </div>
        </div>
        <div class='row'>
          <div class='col s6'>
            <h4>Title: </h4>
            <h5><%= current_user.title %></h5>
          </div>
          <div class='col s6'>
            <h4>About Me: </h4>
            <h5><%= current_user.body %></h5>
          </div>
        </div>
        <div class="row divider">
        </div>
      </div>
    </div>


